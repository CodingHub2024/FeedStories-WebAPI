{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  },
  "AllowedHosts": "*",
  "BaseURI": "https://hacker-news.firebaseio.com/v0/",
  "StoryURL": {
    "GetStoriesURL": "topstories.json?print=pretty",
    "GetStoryDetailsURL": "item/@id.json?print=pretty"
  },
  "HttpPolicy": {
    "RetryNumber": 3,
    "RetryTimeSpan": 5,
    "CircuitBreakerNumber": 5,
    "CircuitBreakerWaitTimeSpan": 2,
    "TimeOut": 15
  }

}
