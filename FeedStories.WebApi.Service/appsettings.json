{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/FeedStories.log",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "AllowedHosts": "*",
  "BaseURI": "https://hacker-news.firebaseio.com/v0/",
  "StoryURL": {
    "StoryIdsURL": "topstories.json?print=pretty",
    "StoryDetailsURL": "item/@id.json?print=pretty"
  },
  "HttpPolicy": {
    "RetryNumber": 3,
    "RetryTimeSpan": 5,
    "CircuitBreakerNumber": 5,
    "CircuitBreakerWaitTimeSpan": 2,
    "TimeOut": 15
  }

}
